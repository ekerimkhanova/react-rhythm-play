{
  "name": "react-rhythm-play",
  "version": "0.0.0",
  "description": "React AudioWave Component",
  "declaration": "es/index.d.ts",
  "engines": {
    "node": ">=8.x"
  },
  "keywords": [
    "react",
    "react-component",
    "react-audio",
    "audio",
    "audioWave",
    "audioVisualization"
  ],
  "homepage": "https://github.com/ekerimkhanova/react-rhythm-play",
  "author": "kerimkhanova93@yandex.ru",
  "repository": {
    "type": "git",
    "url": "https://github.com/ekerimkhanova/react-rhythm-play.git"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/style.scss"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
